                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation=
                              "http://www.attribute-examples.org
                               ProhibitedAttributesExample.xsd">

    <!-- ************************************** -->
    <!--       Shows use of prohibited          -->
    <!-- ************************************** -->

    <glob length="5" height="3" width="1" radius="2" diameter="4"/>


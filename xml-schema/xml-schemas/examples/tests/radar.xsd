<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2000/10/XMLSchema"
            targetNamespace="http://www.sensor.org"
            xmlns="http://www.sensor.org"
            xmlns:t="http://www.test.org"
            elementFormDefault="qualified">
    <xsd:element name="radar">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="sensor" type="t:sensor_type"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>